{
    "background": {
       "scripts": [ "request.js" ]
    },
    "browser_action": {
       "default_icon": "icon-38.png",
       "default_popup": "popup.html"
    },
    "description": "Amazon donates to charity. All you have to do is shop.",
    "icons": { "38": "icon-38.png",
               "128": "icon-128.png" },
    "manifest_version": 2,
    "name": "Smile Wide",
    "permissions": [ "webRequest", "webRequestBlocking", "http://www.amazon.com/*", "https://www.amazon.com/*", "https://smile.amazon.com/*", "http://www.amazon.de/*", "https://www.amazon.de/*", "https://smile.amazon.com/*"],
    "version": "1.73"
}
